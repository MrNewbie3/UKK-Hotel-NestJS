// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Role {
  ADMIN
  RESEPSIONIS
  USER
}

enum Status {
  BARU
  CHECK_IN
  CHECK_OUT
}

model User {
  id   Int    @unique @default(autoincrement())
  uuid String @id @unique
}
